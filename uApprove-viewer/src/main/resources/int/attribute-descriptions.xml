<AttributeResolver>
    <!-- Other stuff here -->



    <!-- SWITCHaai attributes -->

    <!-- Attributes that are mandatory to implement -->

    <!-- E-mail -->
    <resolver:AttributeDefinition id="mail" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="mail">
        <resolver:DisplayName xml:lang="sv">E-postadress</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">E-postadress: E-postadress som används av personen.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Home organization -->
    <resolver:AttributeDefinition id="homeOrganization" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonHomeOrganization">
        <resolver:DisplayName xml:lang="sv">Hemorganisation</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Hemorganisation: Domännamnet för den organisationen som personen tillhör.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Home organization type -->
    <resolver:AttributeDefinition id="homeOrganizationType" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonHomeOrganizationType">
        <resolver:DisplayName xml:lang="sv">Typ av hemorganisation</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Typ av hemorganisation: Vilken organisationstyp hemorganisationen är.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Affiliation -->
    <resolver:AttributeDefinition id="affiliation" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="eduPersonAffiliation">
        <resolver:DisplayName xml:lang="sv">Anknytning</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Anknytning: Vilken anknytning personen har till organisationen.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Unique ID -->
    <resolver:AttributeDefinition id="uniqueID" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonUniqueID">
        <resolver:DisplayName xml:lang="sv">Unik identifierare</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Unik identifierare: En unik identifierare för en person, används primärt för att identifiera personen inloggning vid annan organisation än hemmaorganisationen.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Surname -->
    <resolver:AttributeDefinition id="surname" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="sn">
        <resolver:DisplayName xml:lang="sv">Efternamn</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Efternamn: Efternamn för personen.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Given name -->
    <resolver:AttributeDefinition id="givenName" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="givenName">
        <resolver:DisplayName xml:lang="sv">Förnamn</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Förnamn: Förnamn för personen.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>

    <!-- Attributes that are recommended to implement -->


    <!-- Business postal address -->
    <resolver:AttributeDefinition id="postalAddress" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="postalAddress">
        <resolver:DisplayName xml:lang="sv">Postadress (arbete):</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Postadress (arbete): Postadressen för arbetsplatsen</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Business phone number -->
    <resolver:AttributeDefinition id="telephoneNumber" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="telephoneNumber">
        <resolver:DisplayName xml:lang="sv">Telefonummer (arbete)</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Telefonummer (arbete): Telefonnummer till arbetsplatsen</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Study branch 3 -->
    <resolver:AttributeDefinition id="studyBranch3" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonStudyBranch3">
        <resolver:DisplayName xml:lang="sv">Studieinriktning 3</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Studieinriktning 3: Studieinriktning för studerande.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Study level -->
    <resolver:AttributeDefinition id="studyLevel" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonStudyLevel">
        <resolver:DisplayName xml:lang="sv">Studienivå</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Studienivå: Studienivå för studerandes studieinriktning 3</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Staff category -->
    <resolver:AttributeDefinition id="staffCategory" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonStaffCategory">
        <resolver:DisplayName xml:lang="sv">Personalkategori</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Personalkategori: Arbetsområde för personal</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Matriculation number -->
    <resolver:AttributeDefinition id="matriculationNumber" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonMatriculationNumber">
        <resolver:DisplayName xml:lang="sv">Inskrivningsnummer</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Inskrivningsnummer: Inskrivningsnummer för en studerande.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>

    <!-- Attributes that are optional to implement -->


    <!-- Date of birth -->
    <resolver:AttributeDefinition id="dateOfBirth" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonDateOfBirth">
        <resolver:DisplayName xml:lang="sv">Födelsedata</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Födelsedata: Födelsedata för en person (see RFC 3339).</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Gender -->
    <resolver:AttributeDefinition id="gender" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonGender">
        <resolver:DisplayName xml:lang="sv">Kön</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Kön: Vilket kön en person har (see ISO 5218).</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Preferred Language -->
    <resolver:AttributeDefinition id="preferredLanguage" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="preferredLanguage">
        <resolver:DisplayName xml:lang="sv">Språkönskemål</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Språkönskemål: Personens önskade språk (see RFC1766).</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Home postal address -->
    <resolver:AttributeDefinition id="homePostalAddress" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="homePostalAddress">
        <resolver:DisplayName xml:lang="sv">Postadress (hem)</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Postadress (hem): Postadress till bostaden.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Private phone number -->
    <resolver:AttributeDefinition id="homePhone" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="homePhone">
        <resolver:DisplayName xml:lang="sv">Telefonnummer (hem)</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Telefonnummer (hem): Telefonnummer till bostaden.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Mobile phone number -->
    <resolver:AttributeDefinition id="mobile" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="mobile">
        <resolver:DisplayName xml:lang="en">Telefonnummer (mobil)</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Telefonnummer (mobil): Telefonnummer till mobiltelefon.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Study branch 1 -->
    <resolver:AttributeDefinition id="studyBranch1" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonStudyBranch1">
        <resolver:DisplayName xml:lang="sv">Studieinriktning 1</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Studieinriktning 1: Studieinriktning för studerande, grundläggande nivå.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Study branch 2 -->
    <resolver:AttributeDefinition id="studyBranch2" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="swissEduPersonStudyBranch2">
        <resolver:DisplayName xml:lang="sv">Studieinriktning 2</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Studieinriktning 2: Studieinriktning för studerande, avancerad nivå.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Organization path -->
    <resolver:AttributeDefinition id="orgDN" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="eduPersonOrgDN">
        <resolver:DisplayName xml:lang="sv">DN för organisation</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">DN för organisation: Sökväg (DN) i LDAP-katalogen för till vilken organisation personen tillhör.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Organizational unit path -->
    <resolver:AttributeDefinition id="orgUnitDN" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="eduPersonOrgUnitDN">
        <resolver:DisplayName xml:lang="sv">DN för organisationsenhet</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">DN för organisationsenhet: Sökväg (DN) i LDAP-katalogen för till vilken organisationsenhet personen tillhör.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Entitlement -->
    <resolver:AttributeDefinition id="entitlement" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="eduPersonEntitlement">
        <resolver:DisplayName xml:lang="sv">Rättigheter</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Rättigheter: URI (either URL or URN) som beskriver olika rättigheter till angivna tjänster.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- User ID -->
    <resolver:AttributeDefinition id="uid" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="uid">
        <resolver:DisplayName xml:lang="sv">Användaridentitet</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Användaridentitet: Unik identifierar som används vid lokal inloggning i hemmaorganisationen.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>


    <!-- Employee number -->
    <resolver:AttributeDefinition id="employeeNumber" xsi:type="Simple" xmlns="urn:mace:shibboleth:2.0:resolver:ad" sourceAttributeID="employeeNumber">
        <resolver:Dependency ref="myLDAP" />
        <resolver:DisplayName xml:lang="sv">Anställningsnummer</resolver:DisplayName>
        <resolver:DisplayDescription xml:lang="sv">Anställningsnummer: Unik anställningsidentifierare i hemmaorganisationen.</resolver:DisplayDescription>
    </resolver:AttributeDefinition>

    <!-- Other stuff here -->
</AttributeResolver>
